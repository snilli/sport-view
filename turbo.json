{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"cache": true,
			"outputs": ["dist/**", ".next/**", "!.next/cache/**", "public/dist/**"]
		},
		"build:lib": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		}
	}
}
